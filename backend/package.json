{
  "name": "rmhtc-web-server",
  "version": "1.0.0",
  "description": "node web server for rmhtc web platform",
  "main": "app.js",
  "scripts": {
    "migrate:up": "npx cross-env NODE_ENV=local sequelize-cli db:migrate",
    "migrate:undo": "npx cross-env NODE_ENV=local sequelize-cli db:migrate:undo",
    "start:local": "cross-env NODE_ENV=local nodemon -r dotenv/config app.js",
    "start:dev": "cross-env NODE_ENV=development node -r dotenv/config app.js",
    "start:prod": "cross-env NODE_ENV=production node -r dotenv/config app.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.tronic.website:rmhtc/backend-service.git"
  },
  "keywords": [
    "rmhtc",
    "backend",
    "nodejs",
    "expressjs"
  ],
  "author": "add-team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "minio": "^7.0.27",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.3",
    "sequelize": "^6.16.1",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}
